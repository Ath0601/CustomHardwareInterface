<?xml version="1.0"?>
<robot xmlns:xacro="http://wiki.ros.org/xacro" name="r6bot">

  <!-- Import r6bot macro -->
  <xacro:include filename="$(find description)/urdf/robot_description.urdf.xacro"/>

  <!-- create link fixed to the "world" -->
  <link name="world"/>

  <xacro:robot_description/>

  <!-- Import r6bot ros2_control description -->
  <xacro:include filename="$(find description)/ros2_control/robot.ros2_control.xacro" />
  <xacro:robot_ros2_control name="r6bot"/>

</robot>
